#include <iostream>
#include <vector>

using namespace std;

int main() {
  vector<int> array = {1, 5, 2, 1, 2, 1, 8};
  int numToFind = 5;

  int foundIndex;

  for (int i = 0; i < array.size(); i++) {
    if (array[i] == numToFind) {
      foundIndex = i;
    }
  }
  cout << foundIndex;
  
}





